            android:id="@+id/deepLink"
            app:uri="android-app://com.keepcalling/fragment_one_mt" />
    </fragment>
    <fragment
        android:id="@+id/fragmentTwoMT"
        android:name="com.keepcalling.moduletwo.FragmentTwoMT"
        android:label="FragmentTwoMT" >
