            android:id="@+id/action_fragmentTwoMT_to_fragmentThreeMT"
            app:destination="@id/fragmentThreeMT" />
    </fragment>
    <fragment
        android:id="@+id/fragmentThreeMT"
        android:name="com.keepcalling.moduletwo.FragmentThreeMT"
        android:label="FragmentThreeMT" />
</navigation>